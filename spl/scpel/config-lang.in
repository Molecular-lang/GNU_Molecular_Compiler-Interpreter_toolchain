\$(srcdir)/scpel/parser.h \
\$(srcdir)/c-family/c-common.scpel \$(srcdir)/c-family/c-format.scpel \
\$(srcdir)/c-family/c-cppbuiltin.scpel \$(srcdir)/c-family/c-pragma.scpel \
\$(srcdir)/scpel/call.scpel \$(srcdir)/scpel/class.scpel \$(srcdir)/scpel/constexpr.scpel \
\$(srcdir)/scpel/contracts.scpel \
\$(srcdir)/scpel/constraint.scpel \$(srcdir)/scpel/coroutines.scpel \
\$(srcdir)/scpel/scpel-gimplify.scpel \
\$(srcdir)/scpel/scpel-lang.scpel \$(srcdir)/scpel/scpel-objscpel-common.scpel \
\$(srcdir)/scpel/decl.scpel \$(srcdir)/scpel/decl2.scpel \
