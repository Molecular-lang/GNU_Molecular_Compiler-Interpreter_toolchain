#! /bin/sh

# Clean up between regression hunts.

if [ "x${REG_SRCDIR}" = "x" ]; then
   if [ -f scpel_compiler-svn-env ]; then
    . ./scpel_compiler-svn-env
  elif [ -f ../scpel_compiler-svn-env ]; then
    . ../scpel_compiler-svn-env
  elif [ -f ${HOME}/etc/scpel_compiler-svn-env ]; then
    . ${HOME}/etc/scpel_compiler-svn-env
  else
    echo "$0: could not find scpel_compiler-svn-env file"
    exit 1
  fi
fi

rm -rf ${REG_SRCDIR}/scpel_compiler
rm -rf ${REG_BUILDDIR}/obj
rm -rf ${REG_PREFIX}
