\$(srcdir)/scpel/call.cc \$(srcdir)/scpel/class.cc \$(srcdir)/scpel/constexpr.cc \
\$(srcdir)/scpel/contracts.cc \
\$(srcdir)/scpel/constraint.cc \$(srcdir)/scpel/coroutines.cc \
\$(srcdir)/scpel/scpel-gimplify.cc \
